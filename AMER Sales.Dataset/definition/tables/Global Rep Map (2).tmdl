	column 'PERM Code' =
			
			LOOKUPVALUE(
			    'DF_Sales Rep Lookup (2)'[PERM Code],
			    'DF_Sales Rep Lookup (2)'[WKSalesRep],
			    'Global Rep Map (2)'[WKSalesRep]
			    )
		dataType: string
		lineageTag: 4ba7bd48-2cb7-4b77-bfcb-ceaf7e85d2b7
		summarizeBy: none
		isDataTypeInferred

		annotation SummarizationSetBy = Automatic

	column 'PERM Code NEW' = ```
			
			    VAR CurrentRep =
			    'Global Rep Map (2)'[UserID]
			    VAR RepCode = CALCULATE(
			                    FIRSTNONBLANK('Sales Rep Map'[Rep], 1),
			                        FILTER('Sales Rep Map',
			                            'Sales Rep Map'[Logon] = CurrentRep))
			                            
			
			RETURN
			
			IF(NOT(ISBLANK('Global Rep Map (2)'[PERM Code])),
			    'Global Rep Map (2)'[PERM Code],
			    RepCode)
			```
		dataType: string
		lineageTag: a1c92129-657e-40f0-9c36-ae33fd5db603
		summarizeBy: none
		isDataTypeInferred

