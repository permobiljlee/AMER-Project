			//PastSTB - Division of that period's invoiced by the budget for past quarter's STB
			//QtrBudg - Ascertain current quarter's budget
			//CurrAmountDay - Divide amount invoiced so far by current working day of quarter
			//MultByTotal - Multiply result of CurrAmountDay by total number of working days in current quarter
			//CurrentSTB - Divide result of MultByTotal
			
			VAR First = 
			    DATE(YEAR(TODAY()), QUARTER(TODAY()), 1)
			
			VAR CurrentQ =
			    QUARTER( ( TODAY () ))
			
			VAR PastSTB =
			DIVIDE(
			    CALCULATE([Total Canada (OTHER)]), 
			    CALCULATE([Full Year Budget],
			        FILTER('Budget Master',
			            'Budget Master'[Product Line] IN {"Trushape", "Third Party"})
			        )
			    )
			
			VAR QtrBudg =
			CALCULATE(
			    CALCULATE([Full Year Budget],
			        FILTER('Budget Master',
			            'Budget Master'[Product Line] IN { "Trushape", "Third Party"}),
			        FILTER('Date',
			            'Date'[Quarter] = QUARTER(TODAY()))))
			
			VAR CurrAmountDay =
			DIVIDE(
			    CALCULATE([Total Canada (OTHER)], 
			        FILTER('Date', 
			            'Date'[Quarter] = CurrentQ && 'Date'[year] = 2024)),
			            [Current Working Day - Quarter])
			
			VAR MultByTotal =
			CurrAmountDay * [Total Working Day - Quarter]
			
			VAR CurrentSTB =
			DIVIDE(MultByTotal, QtrBudg)
			
			
			RETURN
			IF(
			    QUARTER(LASTDATE('Date'[Date])) = CurrentQ,
			        CurrentSTB,
			        PastSTB)
			```
		formatString: 0%;-0%;0%
		displayFolder: .STB
		lineageTag: f6ecde52-b41e-4ad3-8cd6-3cf37e8053f3

