			VAR QtrBudg =
			CALCULATE(
			    CALCULATE([Full Year Budget],
			        FILTER('Budget Master',
			            'Budget Master'[Product Line] = "TiLite"),
			        FILTER('Date',
			            'Date'[Quarter] = QUARTER(TODAY()))))
			
			VAR CurrAmountDay =
