	measure 'Invoiced SmartDrives Off-Chair - USSC' =
			
			// CALCULATE(
			//     SUM('Facts'[Net price]),
			//         FILTER('Facts',
			//         'Facts'[Company_SK] IN {"DBF107EA99A53AFD092321B962232EFB81FF9297"} && // USSC
			//         'Facts'[Order type] IN {"SMRT", "Parts"} &&
			//         'Facts'[FinProdDesc 1] IN {"Aftermarket", "Manual"} &&
			//         NOT CONTAINSSTRING('Facts'[FinProdDesc 2], "Power") &&
			//         NOT CONTAINSSTRING('Facts'[FinProdDesc 2], "Services")
			//     )
			// )
			
			CALCULATE (
			    SUM ( 'Facts'[Net price] ),
			    'Date'[date_int] >= 20220401,
			    'Facts'[Company_SK] IN { "DBF107EA99A53AFD092321B962232EFB81FF9297" },
			    -- USSC
			    'Facts'[Order Type - Calculated] IN { "SMRT" },
			    NOT 'Facts'[ItemNumber] IN { "Freight", "RestockExt" },
			    -- expect filter to exclude all Frieght item numbers and RestockExt item numbers typically for restock fees on Returned Orders
			    'Bill to customer'[Bill to internal customer]
			        IN {
			            0
			        } -- using Bill to instead of Ship to as often Returned Orders don't have a Ship to customer number
			)
