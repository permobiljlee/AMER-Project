	partition 'Open Quotes PWC - Historical' = calculated
		mode: import
		source =
				
				SELECTCOLUMNS(
				    GENERATE(
				        'OR01_QUOTE_CONV_UNION_View',
				        FILTER(
				            ALLNOBLANKROW('Date'),
				            AND(
				                'Date'[Date] >= 'OR01_QUOTE_CONV_UNION_View'[New Quote Date],
				                'Date'[Date] <= 'OR01_QUOTE_CONV_UNION_View'[Order Date Key]
				            )
				        )
				    ),
				    "Date Key", 'Date'[Date],
				    "Order Number", 'OR01_QUOTE_CONV_UNION_View'[Order_Number],
				    "Date (Quote)", 'OR01_QUOTE_CONV_UNION_View'[New Quote Date],
				    "Dealer Code", 'OR01_QUOTE_CONV_UNION_View'[Dealer_Code],
				    "Product Code", 'OR01_QUOTE_CONV_UNION_View'[Product Code],
				    // "Chair Base", OR01_QUOTE_CONV_UNION_View[Chair base],
				    "Quote Count", 'OR01_QUOTE_CONV_UNION_View'[Quote_Count],
				    "Quote Age", DATEDIFF( 'OR01_QUOTE_CONV_UNION_View'[New Quote Date], 'Date'[Date], DAY)
				)

	annotation PBI_Id = 4806897725d54c1698d6fe5cf536b2d3

