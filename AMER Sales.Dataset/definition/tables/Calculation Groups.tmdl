table 'Calculation Groups'
	lineageTag: 86991e98-225c-4f88-9fbc-7a5e984bfd25

	calculationGroup

		calculationItem 'Full Year Budget' = CALCULATE(SELECTEDMEASURE())

		calculationItem 'Q1 Budget' = ```
				CALCULATE(SELECTEDMEASURE(), 
				    FILTER('Date',
				        'Date'[Quarter] = 1))
				```

		calculationItem 'Q2 Budget' = ```
				CALCULATE(SELECTEDMEASURE(), 
				    FILTER('Date',
				        'Date'[Quarter] = 2))
				```

		calculationItem 'Q3 Budget' = ```
				CALCULATE(SELECTEDMEASURE(), 
				    FILTER('Date',
				        'Date'[Quarter] = 3))
				```

		calculationItem 'Q4 Budget' = ```
				CALCULATE(SELECTEDMEASURE(), 
				    FILTER('Date',
				        'Date'[Quarter] = 4))
				```

		calculationItem 'PWC STB Q1' = ```
				VAR div =
				DIVIDE(
				    CALCULATE([Invoiced Power Chairs - US], FILTER('Date', 'Date'[Quarter] = 1 && 'date'[year] = 2023)),
				    [Current Working Day - Quarter])
				VAR mult = 
				div * [Total Working Day - Quarter]
				VAR final =
				DIVIDE(
				    mult,
				    CALCULATE(SELECTEDMEASURE(), 
				    FILTER('Date',
				        'Date'[Quarter] = 1),
				    FILTER('Master Budget 2023',
				        'Master Budget 2023'[Product Line] = "Power Chairs")))
				RETURN
				final
				```

		calculationItem 'SD STB Q1' = ```
				VAR div =
				DIVIDE(
				    CALCULATE([Invoiced TiLite On-Chair - SmartDrives Aluminum] + [Invoiced SmartDrives Off-Chair for Commissions - Total US], FILTER('Date', 'Date'[Quarter] = 1 && 'date'[year] = 2023)),
				    [Current Working Day - Quarter])
				VAR mult = 
				div * [Total Working Day - Quarter]
				VAR final =
				DIVIDE(
				    mult,
				    CALCULATE(SELECTEDMEASURE(), 
				    FILTER('Date',
				        'Date'[Quarter] = 1),
				    FILTER('Master Budget 2023',
				        'Master Budget 2023'[Product Line] = "SmartDrive On-Chair" ||
				        'Master Budget 2023'[Product Line] = "SmartDrive On-Chair")))
				RETURN
				final
				```

